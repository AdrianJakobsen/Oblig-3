package implementation;

public class textReverse {

	public void revereseString(String stringToReverse) {
		int n = stringToReverse.length();
		if(n <= 1){
			System.out.print(stringToReverse.charAt(0));
			
		}else{
			
			revereseString(stringToReverse.substring(0, stringToReverse.length()));
		}
	}

}
